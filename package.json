{
  "name": "mithril-scenario-timeline",
  "version": "0.0.1",
  "description": "A scenario timeline component for the Mithril framework",
  "main": "index.js",
  "scripts": {
    "postinstall": "pnpm run link",
    "link": "pnpm m run link && pnpm m run postlink",
    "clean": "rm -fr ./docs && pnpm run clean --parallel",
    "build:domain": "pnpm run build:domain --parallel",
    "start": "pnpm run start --parallel",
    "patch-release": "pnpm run patch-release --parallel",
    "minor-release": "pnpm run minor-release --parallel",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erikvullings/mithril-scenario-timeline.git"
  },
  "keywords": [
    "mithril",
    "scenario",
    "timeline",
    "mithril",
    "component"
  ],
  "author": "Erik Vullings <erik.vullings@gmail.com> (http://www.tno.nl)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/erikvullings/mithril-scenario-timeline/issues"
  },
  "homepage": "https://github.com/erikvullings/mithril-scenario-timeline#readme",
  "devDependencies": {
    "pnpm": "^5.10.4"
  }
}